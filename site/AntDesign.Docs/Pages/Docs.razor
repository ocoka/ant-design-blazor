@page "/docs/{fileName}"
@page "/docs"
<section class="main-container">
    <article class="markdown">
        @if (_file != null)
        {
            var title = $"{_file.Title} - Ant Design of Blazor";
            <Microsoft.AspNetCore.Components.Web.Extensions.Head.Title Value="@title" />
            <h1>@_file.Title</h1>
            @((MarkupString) _file.Html)
        }
    </article>
</section>
